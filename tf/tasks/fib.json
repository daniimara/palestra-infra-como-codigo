[
    {
      "name": "fib",
      "image": "${image}",
      "portMappings": [
        {
          "containerPort": 9090,
          "hostPort": 9090
        }
      ],
      "memory": 512,
      "networkMode": "awsvpc",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "${log_group}",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "loccity_api"
        }
      },
      "environment": [
        {
          "name": "FOO",
          "value": "bar"
        }
      ]
    }
  ]